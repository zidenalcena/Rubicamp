<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
      crossorigin="anonymous"
    />
    <title>Edit Data</title>
  </head>
  <body>
    <div class="container" style="padding: 10px">
      <h1>Edit Data</h1>
      <form action="/edit/<%= data.ID %>" method="POST">
        <div class="form-group row">
          <label class="col-sm-2 col-form-label">ID</label>
          <div class="col-sm-8">
            <input
              type="text"
              class="form-control"
              id="formGroupExampleInput"
              value="<%= data.ID %>"
              disabled
            />
          </div>
        </div>
        <div class="form-group row">
          <label class="col-sm-2 col-form-label">String</label>
          <div class="col-sm-8">
            <input
              type="text"
              class="form-control"
              id="String"
              name="String"
              value="<%= data.String %>"
            />
          </div>
        </div>
        <div class="form-group row">
          <label class="col-sm-2 col-form-label">Integer</label>
          <div class="col-sm-8">
            <input
              type="text"
              class="form-control"
              id="Integer"
              name="Integer"
              value="<%= data.Integer %>"
            />
          </div>
        </div>
        <div class="form-group row">
          <label class="col-sm-2 col-form-label">Float</label>
          <div class="col-sm-8">
            <input
              type="text"
              class="form-control"
              id="Float"
              name="Float"
              value="<%= data.Float %>"
            />
          </div>
        </div>
        <div class="form-group row">
          <label class="col-sm-2 col-form-label">Date</label>
          <div class="col-sm-8">
            <input
              type="date"
              class="form-control"
              id="Date"
              name="Date"
              value="<%= data.Date %>"
            />
          </div>
        </div>
        <div class="form-group row">
          <label class="col-sm-2 col-form-label">Float</label>
          <div class="col-sm-8">
            <select
              id="inputState"
              class="form-control"
              type="boolean"
              name="Boolean"
              id="Boolean"
            >
              <option value="a" <%= data.Boolean.toString() == "true" ? "selected" : "" %>>true</option>
              <option value="" <%= data.Boolean.toString() == "false" ? "selected" : "" %>>false</option>
            </select>
          </div>
        </div>
        <button type="submit" class="btn btn-info">Update</button>
      </form>
    </div>
  </body>
</html>
